{
  "name": "coffee-shop",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@popperjs/core": "^2.11.5",
    "@react-hook/window-size": "^3.0.7",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "13.5.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "27.4.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "16.11.26",
    "@types/react": "^17.0.44",
    "@types/react-dom": "17.0.15",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-mock-store": "^1.0.3",
    "@types/styled-components": "^5.1.25",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "axios": "^0.26.1",
    "babel-jest": "^27.5.1",
    "customize-cra": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "jest": "^27.5.1",
    "react": "^17.0.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^17.0.0",
    "react-popper": "^2.2.5",
    "react-redux": "^8.0.0",
    "react-responsive-carousel": "^3.2.23",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "react-test-renderer": "^17.0.2",
    "redux": "^4.1.2",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^27.1.4",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "jest",
    "test-update": "jest --updateSnapshot",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "setupFiles": [
      "./src/tests/setup/test-shim.js",
      "./src/tests/setup/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "preset": "ts-jest/presets/js-with-ts",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/setup/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/tests/setup/styleMock.js"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testMatch": [
      "**/tests/**/*.test.(ts|tsx|js)"
    ],
    "collectCoverage": false
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^2.0.7",
    "jest-styled-components": "^7.0.8",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "lodash.debounce": "^4.0.8",
    "prettier": "2.6.2",
    "styled-components": "^5.3.5",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
